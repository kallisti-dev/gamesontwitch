@require 'colors'

.queue-title-row
    margin-bottom: 20px
    padding-bottom: 5px
    border-bottom: 1px solid border-color
    min-width: 634px
    
.queue-title-col
    .queue-title
        margin: 0
    .button
        width: 82px
        height: 25px
        vertical-align: text-bottom
    padding-left: 5px
    padding-right: 20px
    display: inline-block
    
.queue-header
    margin-top: 14px
    
.queue
    background-color: bg2
    min-width: 634px
    max-width: 860px

.queue-contents
    margin-top: 0
    overflow: hidden
    padding-left: 0
    list-style-type: none
    
.queue-row
    position: relative
    overflow: hidden
    white-space: nowrap
    list-style-position: inside
    padding-top: 5px
    border-bottom: 1px solid border-color
    height:30px
   
.queue-row.highlight
    background: bg3
   
.queue-col
    display: inline-block
    min-width: 100px
    padding-left: 5px
    padding-right: 5px
    .col-text
        position: relative
        top: 8px
    
.profile-col
    min-width: 220px
    
.queue-delete-confirm
    p
        padding-bottom: 20px
    .button
        width: 80px
        height: 25px
    height: 50px
    width: 300px
    text-align: center
    